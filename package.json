  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "node scripts/dev-fast.js",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
